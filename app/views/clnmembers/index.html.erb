<h1>Listing clnmembers</h1>

<table>
  <tr>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Phone no</th>
    <th>Company name</th>
    <th>Occupation</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @clnmembers.each do |clnmember| %>
  <tr>
    <td><%= clnmember.email %></td>
    <td><%= clnmember.first_name %></td>
    <td><%= clnmember.last_name %></td>
    <td><%= clnmember.phone_no %></td>
    <td><%= clnmember.company_name %></td>
    <td><%= clnmember.occupation %></td>
    <td><%= link_to 'Show', clnmember %></td>
    <td><%= link_to 'Edit', edit_clnmember_path(clnmember) %></td>
    <td><%= link_to 'Destroy', clnmember, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

